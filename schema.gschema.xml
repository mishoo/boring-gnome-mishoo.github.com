<schemalist>
  <schema id="org.gnome.shell.extensions.mishoo.boring-gnome" path="/org/gnome/shell/extensions/mishoo/boring-gnome/">
    <key type="as" name="cycle-workspaces">
      <default><![CDATA[['<Super>Tab','<Ctrl><Alt>Tab']]]></default>
      <summary>Cycle between the last two workspaces</summary>
    </key>
    <key type="as" name="cycle-workspaces-take-window">
      <default><![CDATA[['<Shift><Super>Tab','<Shift><Ctrl><Alt>Tab']]]></default>
      <summary>Cycle between the last two workspaces taking current window</summary>
    </key>
    <key type="as" name="find-window-by-name">
      <default><![CDATA[['<Ctrl><Super>r', '<Ctrl><Super>z']]]></default>
      <summary>Display a list of all windows (all workspaces) allowing you
      to quickly seacrh through it.</summary>
    </key>
    <key type="as" name="rotate-window-right">
      <default><![CDATA[['<Ctrl><Super>Page_Down']]]></default>
      <summary>Rotate window right</summary>
    </key>
    <key type="as" name="rotate-window-left">
      <default><![CDATA[['<Ctrl><Super>Page_Up']]]></default>
      <summary>Rotate window left</summary>
    </key>
    <key type="as" name="zoom-in-window">
      <default><![CDATA[['<Super>Page_Up']]]></default>
      <summary>Zoom in window</summary>
    </key>
    <key type="as" name="zoom-out-window">
      <default><![CDATA[['<Super>Page_Down']]]></default>
      <summary>Zoom out window</summary>
    </key>
    <key type="as" name="reset-window-zoom-and-scale">
      <default><![CDATA[['<Super>Home']]]></default>
      <summary>Zoom out window</summary>
    </key>
  </schema>
</schemalist>
